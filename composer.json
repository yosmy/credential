{
    "name": "yosmy/credential",
    "description": "",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "yosmy/jwt": "@dev",
        "yosmy/phone": "@dev",
        "yosmy/privilege": "@dev"
    },
    "require-dev": {
        "symfony/debug": "~4",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Yosmy\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yosmy\\Test\\": [
                "tests"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://php.yosmy.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
